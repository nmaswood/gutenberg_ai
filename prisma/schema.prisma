datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL") 
}

generator client {
  provider = "prisma-client-js"
}

model Book {
  id            Int     @id
  title         String
  authors       String
  subjects      String
  bookshelves   String
  languages     String
  mediaType     String  @map("media_type")
  downloadCount Int     @map("download_count")
  textContent   String? 
  textPath      String? 
}
